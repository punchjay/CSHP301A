<Window x:Class="VendingMachineWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VendingMachineWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Insert Your Coins Here!" HorizontalAlignment="Center" Margin="0,21,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="SemiBold"/>
        <Button HorizontalAlignment="Left" Margin="140,151,0,0" VerticalAlignment="Top">
            <Image Height="100" Width="100"/>
        </Button>
        <Button HorizontalAlignment="Center" Margin="0,151,0,0" VerticalAlignment="Top">
            <Image Height="100" Width="100"/>
        </Button>
        <Button HorizontalAlignment="Right" Margin="0,151,140,0" VerticalAlignment="Top">
            <Image Height="100" Width="100"/>
        </Button>
        <TextBlock HorizontalAlignment="Left" Margin="149,306,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="369,306,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="583,306,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button x:Name="ButtonCoinReturn" Content="Button" HorizontalAlignment="Left" Margin="717,307,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonInsertHalftDollar" Content="Button" HorizontalAlignment="Left" Margin="231,364,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonInsertQuarter" Content="Button" HorizontalAlignment="Left" Margin="331,364,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtontnsertDime" Content="Button" HorizontalAlignment="Left" Margin="427,364,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonInsertNickel" Content="Button" HorizontalAlignment="Left" Margin="525,364,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
